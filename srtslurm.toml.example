# SRT SLURM Configuration
# Copy this to srtslurm.toml and fill in your values

[cluster]
# SLURM cluster settings - these are defaults that can be overridden via CLI
account = "restricted"               # SLURM account
partition = "batch-aqua"             # SLURM partition
network_interface = "enP6p9s0np0"    # Network interface 
time_limit = "4:00:00"               # Default job time limit

[cloud]
# S3-compatible cloud storage for syncing benchmark results
# Leave empty to disable cloud sync
endpoint_url = ""                    # e.g., "https://s3.us-west-2.amazonaws.com"
bucket = ""                          # e.g., "my-benchmark-results"
prefix = "benchmark-results/"        # Optional prefix for all keys

# Credentials should be set as environment variables:
# export AWS_ACCESS_KEY_ID="your-access-key"
# export AWS_SECRET_ACCESS_KEY="your-secret-key"

